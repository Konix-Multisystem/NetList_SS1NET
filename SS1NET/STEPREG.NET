COMPILE;
DIRECTORY MASTER;
/****************************************************************************
stepreg
10/10/88

Address update step value register

****************************************************************************/


MODULE STEPREG;

INPUTS	ID_0,ID_1,ID_2,ID_3,ID_4,ID_5,ID_6,ID_7,LDMODL,LDSTPL;
OUTPUTS	STEP_0,STEP_1,STEP_2,STEP_3,STEP_4,STEP_5,STEP_6,STEP_7,STEPM1,YFRAC;
LEVEL FUNCTION;
DEFINE

STPDL_0_(STEP_0,STEPL_0) = LD2A(ID_0,LDSTPL);
STPDL_1_(STEP_1,STEPL_1) = LD2A(ID_1,LDSTPL);
STPDL_2_(STEP_2,STEPL_2) = LD2A(ID_2,LDSTPL);
STPDL_3_(STEP_3,STEPL_3) = LD2A(ID_3,LDSTPL);
STPDL_4_(STEP_4,STEPL_4) = LD2A(ID_4,LDSTPL);
STPDL_5_(STEP_5,STEPL_5) = LD2A(ID_5,LDSTPL);
STPDL_6_(STEP_6,STEPL_6) = LD2A(ID_6,LDSTPL);
STPDL_7_(STEP_7,STEPL_7) = LD2A(ID_7,LDSTPL);
STPM1L_(STEPM1,STEPM1L) = LD2A(ID_0,LDMODL);

YFRL_(YFRAC,YFRACL) = LD2A(ID_4,LDMODL);

END MODULE;
END COMPILE;
END;
