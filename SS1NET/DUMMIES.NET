COMPILE;
DIRECTORY MASTER;

MODULE MUX16;

INPUTS	A_0,A_1,A_2,A_3,EN;
OUTPUTS	RO2_0,RO2_1,RO2_2,RO2_3,RO2_4,RO2_5,RO2_6,RO2_7,RO2_8,RO2_9,RO2_10,
   RO2_11,RO2_12,RO2_13,RO2_14,RO2_15;
LEVEL FUNCTION;
DEFINE

/* Address decoding */


RO1_(RO1_0,RO1_1,RO1_2,RO1_3) = DEMUX(A_2,A_3,EN);
RO2_0_(RO2_0,RO2_1,RO2_2,RO2_3) = DEMUX(A_0,A_1,RO1_0);
RO2_4_(RO2_4,RO2_5,RO2_6,RO2_7) = DEMUX(A_0,A_1,RO1_1);
RO2_8_(RO2_8,RO2_9,RO2_10,RO2_11) = DEMUX(A_0,A_1,RO1_2);
RO2_12_(RO2_12,RO2_13,RO2_14,RO2_15) = DEMUX(A_0,A_1,RO1_3);


END MODULE;


MODULE DEMUX;

INPUTS	I_0,I_1,EN;
OUTPUTS	O_0,O_1,O_2,O_3;
LEVEL FUNCTION;
DEFINE

O_3_(O_3) = AND3A(I_1,I_0,EN);
O_2_(O_2) = AND3A(I_1,IL_0,EN);
O_1_(O_1) = AND3A(IL_1,I_0,EN);
O_0_(O_0) = AND3A(IL_1,IL_0,EN);

IL_0_(IL_0) = N1A(I_0);
IL_1_(IL_1) = N1A(I_1);
END MODULE;

END COMPILE;
END;
